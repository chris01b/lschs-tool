{
  "manifest_version": 2,

  "name": "LSCHS Tool",
  "description": "This extension allows the user to view letter day for school.",
  "version": "1.2",

  "browser_action": {
    "default_icon": {
        "16": "icons/rsz_16.png",
        "48": "icons/rsz_48.png",
        "128": "icons/rsz_128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Check the schedule"
  },
  "permissions": [
    "activeTab"
  ]
}